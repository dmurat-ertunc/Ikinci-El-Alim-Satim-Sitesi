                    <span class="text-black text-black-50" asp-validation-for="KategoriAd"></span>
                    @foreach (var item in Model.kategorilers)
                    {
                        <option value="@item.Id" slug="@item.Id">@item.KategoriAd</option>
                    }
                </select>
            </div>
            <div class="mb-3 input-field">
                <label class="form-label">Ürün Çeşiti Seçiniz</label>
