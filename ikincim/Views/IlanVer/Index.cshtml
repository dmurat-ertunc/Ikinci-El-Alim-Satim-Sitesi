                <label class="form-label">Kullanıcı Adı</label>
                <input type="text" class="form-control input" value="@User.Identity.Name" readonly>
            </div>
            <div class="mb-3 input-field">
                <label class="form-label">İlanın Adı</label>
                <input type="text" class="form-control input" asp-for="IlanAd">
                <span class="text-black text-black-50" asp-validation-for="IlanAd"></span>
            </div>
            <div class="mb-3 input-field">
                <label for="comment">İlan Açıklaması</label>
                <textarea class="form-control input" rows="15" asp-for="Aciklama"></textarea>
                <span class="text-black text-black-50" asp-validation-for="Aciklama"></span>
            </div>
            <div class="mb-3 input-field">
                <label class="form-label">Ürünün Yılı</label>
                <input type="number" class="form-control input" asp-for="Yil">
                <span class="text-black text-black-50" asp-validation-for="Yil"></span>
            </div>
            <div class="mb-3 input-field">
                <label class="form-label">Fiyat</label>
                <input type="number" class="form-control input" asp-for="Fiyat">
                <span class="text-black text-black-50" asp-validation-for="Fiyat"></span>
            </div>
        </div>
